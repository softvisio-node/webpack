{
    "name": "@softvisio/webpack",
    "version": "1.0.73",
    "description": "Webpack wrapper",
    "keywords": [
        "softvisio",
        "webpack"
    ],
    "homepage": "https://softvisio-node.github.io/webpack/",
    "bugs": {
        "url": "https://github.com/softvisio-node/webpack/issues",
        "email": "root <root@softvisio.net>"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/softvisio-node/webpack.git"
    },
    "license": "ISC",
    "author": "root <root@softvisio.net>",
    "type": "module",
    "imports": {
        "#core/*": "@softvisio/core/*",
        "#lib/*": "./lib/*.js",
        "#resources/*": "./resources/*"
    },
    "exports": {
        ".": "./lib/webpack.js",
        "./*": "./lib/*.js",
        "./resources/*": "./resources/*",
        "./webpack-components/*": "./resources/webpack-components/*/component.js"
    },
    "bin": {
        "softvisio-webpack": "bin/softvisio-webpack.js"
    },
    "files": [
        "/bin",
        "/lib",
        "/resources",
        "/src"
    ],
    "scripts": {
        "install": "node bin/install.js"
    },
    "peerDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/plugin-transform-runtime": "^7.15.8",
        "@babel/preset-env": "^7.15.6",
        "@softvisio/core": "^7.0.0",
        "babel-loader": "^9.0.1",
        "copy-webpack-plugin": "^12.0.0",
        "core-js": "^3.22.2",
        "css-loader": "^7.0.0",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "html-webpack-plugin": "^5.3.2",
        "mini-css-extract-plugin": "^2.0.0",
        "postcss-loader": "^8.0.0",
        "terser-webpack-plugin": "^5.2.4",
        "vue": "^3.4.30",
        "vue-loader": "^17.0.0",
        "vue-style-loader": "^4.1.3",
        "webpack": "^5.55.0",
        "webpack-bundle-analyzer": "^4.4.2",
        "webpack-dev-server": "^5.0.0",
        "webpack-preprocessor-loader": "^1.2.0"
    }
}
